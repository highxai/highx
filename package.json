{
	"name": "highx",
	"version": "0.1.10",
	"description": "The ultimate gateway for AI apps",
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"cli": {
		"highx": "./dist/cli.js"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "sh ./build.sh",
		"project:build": "bunup",
		"dev": "bunup --watch",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format .",
		"format:fix": "biome format --write .",
		"tsc": "tsc --noEmit",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"test:coverage": "bun test --coverage",
		"release": "bumpp --commit --push --tag",
		"publish:ci": "bun publish --access public --no-git-checks",
		"prepare": "husky"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.2",
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@types/bun": "^1.2.17",
		"@types/ink": "^2.0.3",
		"@types/react": "^19.1.8",
		"bumpp": "^10.2.0",
		"bunup": "^0.8.51",
		"husky": "^9.1.7",
		"typescript": "^5.8.3"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/highxai/highx.git"
	},
	"peerDependencies": {
		"typescript": ">=4.5.0"
	},
	"peerDependenciesMeta": {
		"typescript": {
			"optional": true
		}
	},
	"homepage": "https://highx.dev",
	"bugs": {
		"url": "https://github.com/highxai/highx/issues"
	},
	"license": "Apache-2.0",
	"dependencies": {
		"gradient-string": "^3.0.0",
		"ink": "^6.0.1",
		"ink-select-input": "^6.2.0",
		"react": "^19.1.0",
		"zod": "^3.25.74"
	}
}
